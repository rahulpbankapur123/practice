name: my first workflow

on: push

jobs: 
    first_job:
        runs-on: ubuntu-latest
        steps:
        - name: Checkout Repo
          uses: actions/checkout@v4           
          
        - name: welcome message
          run: echo "my first github actions job"

        - name: list files
          run: ls

        - name: Read the file
          run: cat README.md
